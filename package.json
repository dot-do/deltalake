{
  "name": "@dotdo/deltalake",
  "version": "0.0.1",
  "description": "Delta Lake format implementation - shared foundation for MongoLake and KafkaLake",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./parquet": {
      "types": "./dist/parquet/index.d.ts",
      "import": "./dist/parquet/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/query/index.js"
    },
    "./cdc": {
      "types": "./dist/cdc/index.d.ts",
      "import": "./dist/cdc/index.js"
    },
    "./delta": {
      "types": "./dist/delta/index.d.ts",
      "import": "./dist/delta/index.js"
    },
    "./compaction": {
      "import": "./dist/compaction/index.js",
      "types": "./dist/compaction/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:coverage": "vitest run --config vitest.unit.config.ts --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:conformance": "vitest run --config vitest.conformance.config.ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "prepublishOnly": "npm run typecheck && vitest run",
    "download:dat": "bash scripts/download-dat.sh"
  },
  "dependencies": {
    "@dotdo/hyparquet": "^1.17.3",
    "@dotdo/hyparquet-writer": "^0.12.1",
    "hyparquet-compressors": "^1.1.1"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20250124.0",
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "^3.0.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^3.100.0"
  },
  "keywords": [
    "delta-lake",
    "parquet",
    "lakehouse",
    "streaming",
    "cdc",
    "cloudflare-workers"
  ],
  "author": "dotdo",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/deltalake"
  },
  "bugs": {
    "url": "https://github.com/dot-do/deltalake/issues"
  },
  "homepage": "https://github.com/dot-do/deltalake#readme"
}
